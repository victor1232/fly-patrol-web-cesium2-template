{
  "name": "security-enterprise-admin",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "npm run build:prod",
    "build:prod": "vue-tsc --noEmit && vite build --mode production",
    "build:sit": "vue-tsc --noEmit && vite build --mode production.sit",
    "build:uat": "vue-tsc --noEmit && vite build --mode production.uat",
    "serve": "npm run build && vite preview",
    "lint": "eslint \"src/**/*.{vue,ts}\" --fix"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{ts,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@tinymce/tinymce-vue": "4.0.3",
    "@vueuse/core": "^5.0.3",
    "ajv": "^6.12.6",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "core-js": "^3.14.0",
    "echarts": "^5.1.2",
    "element-plus": "1.0.2-beta.70",
    "jsts": "2.7.1",
    "lodash": "^4.17.21",
    "mitt": "^2.1.0",
    "monaco-editor": "^0.25.2",
    "nprogress": "^0.2.0",
    "ol": "^6.9.0",
    "qs": "^6.10.1",
    "quill": "^1.3.7",
    "sortablejs": "^1.13.0",
    "vue": "^3.1.5",
    "vue-echarts": "^6.0.0-rc.6",
    "vue-i18n": "9.1.7",
    "vue-router": "4.0.11",
    "vuex": "4.0.2",
    "vxe-table": "4.0.23",
    "webpack": "^5.0.0",
    "wkt": "^0.1.1",
    "xe-utils": "3.3.0",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.172",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.6",
    "@types/quill": "^2.0.8",
    "@types/sortablejs": "^1.10.6",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "@vitejs/plugin-vue": "^1.4.0",
    "@vue/compiler-sfc": "^3.2.1",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "eslint": "^7.28.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.11.1",
    "less": "^4.1.1",
    "less-loader": "^10.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.1",
    "typescript": "~4.3.3",
    "vite": "2.4.4",
    "vite-plugin-eslint": "^1.1.3",
    "vite-plugin-html": "^2.0.7",
    "vite-plugin-pwa": "^0.8.1",
    "vite-plugin-svg-icons": "^0.7.1",
    "vite-tsconfig-paths": "^3.3.13",
    "vue-tsc": "^0.2.2",
    "yorkie": "^2.0.0"
  }
}
